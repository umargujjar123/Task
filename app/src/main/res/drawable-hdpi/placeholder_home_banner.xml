<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:radius="16px"/>
            <stroke android:color="@color/splash_background" android:width="0.3dp"/>
        </shape>
    </item>
    <item
        android:width="40dp"
        android:height="40dp"
        android:gravity="center"
        android:left="60dp"
        android:right="60dp"
        android:top="20dp"
        android:bottom="20dp"
        android:drawable="@drawable/ic_latest_news" />

</layer-list>